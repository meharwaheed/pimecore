    {% if not document is defined or not document %}
        {% set document = pimcore_document(1) %}
    {% endif %}

    {% if document is instanceof('\\Pimcore\\Model\\Document\\Link') %}
        {# @var document \Pimcore\Model\Document\Link #}
        {% set document = document.getObject() %}
    {% endif %}

    {% if document.getTitle() is not empty %}
        {% do pimcore_head_title().set(document.getTitle()) %}
    {% endif %}

    {% if document.getDescription() is not empty %}
        {% do pimcore_head_meta().setDescription(document.getDescription()) %}
    {% endif %}

    {% do pimcore_head_title().append(pimcore_website_config('page-title')) %}
    {% do pimcore_head_title().setSeparator(' - ') %}

    {% for key, lang in get_localized_links(document) %}
        {% do pimcore_head_link().appendAlternate("#{lang.link}", null, null, {"hreflang": "#{key}"}) %}
    {% endfor %}

